{
  "manifest_version": 2,
  "name": "Quick Link Extension",
  "version": "0.1.2",
  "permissions": ["activeTab"],
  "commands": {
    "YouVersion": {
      "suggested_key": "Alt+Q",
      "description": "YouVersion"
    },
    "BollsLife": {
      "suggested_key": "Alt+W",
      "description": "Bolls.Life"
    }
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "cross.png"
  },
  "icons": {
    "16": "cross.png",
    "48": "cross.png",
    "128": "cross.png"
  }
}
